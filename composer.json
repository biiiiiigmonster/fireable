{
    "name": "biiiiiigmonster/laravel-fireable",
    "description": "Laravel framework eloquent trait",
    "type": "library",
    "keywords": ["laravel", "orm", "trait"],
    "license": "MIT",
    "authors": [
        {
            "name": "YunFeng Lu",
            "email": "603707288@qq.com"
        }
    ],
    "require": {
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "phpunit/phpunit": "^9",
        "orchestra/testbench": "^7.7",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "BiiiiiigMonster\\Fireable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BiiiiiigMonster\\Fireable\\Tests\\": "tests",
            "BiiiiiigMonster\\Fireable\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BiiiiiigMonster\\Fireable\\FiresServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
